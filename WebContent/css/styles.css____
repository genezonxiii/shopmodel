.panel-content .f-label { display: inline-block; font-weight: bold; width: 120px; }

.panel-content .f-label-inline { font-weight: bold; width: auto; margin-left: 32px; }

.datalistWrap { margin: 25px 25px 15px; }

input[type="text"], textarea { background: #fff; border: 1px solid #999; font-size: 15px;}

input[type="text"]:focus, textarea:focus { background: #d8f5fa; border: 1px solid #333; }

.aRight { float: right; }

.aCenter { text-align: center; }

.row { border-bottom: 1px solid #ccc; margin-bottom: 15px; padding-bottom: 10px; width: 100%; zoom: 1; }

.row:before, .row:after { content: ''; display: table; }

.row:after { clear: both; }

.formTable { border-collapse: separate; width: 100%; }

.formTable thead { font-weight: bold; }

.formTable thead th { background: #14145E; color: white; padding: 3px; text-align: center;}

.formTable tbody tr { background: #effbfd; }

.formTable tbody tr:nth-child(2n+1) { background: #d8f5fa; }

.formTable tbody td { padding: 8px 3px; }

/*-- Buttons --*/
.btn-control { padding-top: 8px; margin: 25px 0 10px; zoom: 1; }

.btn-control:before, .btn-control:after { content: ''; display: table; }

.btn-control:after { clear: both; }

.btn { display: inline-block; *display: inline; margin-bottom: 10px; *margin-left: .3em; margin-right: 6px; font-size: 15px; line-height: 21px; color: #333333; text-align: center; text-decoration: none; vertical-align: middle; cursor: pointer; background-color: #f5f5f5; *background-color: #e6e6e6; background-repeat: repeat-x; border: 0; *border: 1px solid #ccc; *zoom: 1; background-color: #aaa; /* Fallback Color */ background-image: -webkit-gradient(linear, left top, left bottom, from(#ccc), to(#aaa)); /* Saf4+, Chrome */ background-image: -webkit-linear-gradient(top, #ccc, #aaa); /* Chrome 10+, Saf5.1+, iOS 5+ */ background-image: -moz-linear-gradient(top, #ccc, #aaa); /* FF3.6 */ background-image: -ms-linear-gradient(top, #ccc, #aaa); /* IE10 */ background-image: -o-linear-gradient(top, #ccc, #aaa); /* Opera 11.10+ */ background-image: linear-gradient(top, #ccc, #aaa); -webkit-border-radius: 3px; -moz-border-radius: 3px; border-radius: 3px; -webkit-box-shadow: 0 2px 0 0 #999; -moz-box-shadow: 0 2px 0 0 #999; box-shadow: 0 2px 0 0 #999; /* -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=180, Color='#000000')"; */ -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#c0c0c0')"; filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#c0c0c0'); }

.btn:hover, .btn:focus, .btn.disabled { color: #333333; background-color: #bbb; text-decoration: none; background-color: #ccc; /* Fallback Color */ background-image: -webkit-gradient(linear, left top, left bottom, from(#bbb), to(#ccc)); /* Saf4+, Chrome */ background-image: -webkit-linear-gradient(top, #bbb, #ccc); /* Chrome 10+, Saf5.1+, iOS 5+ */ background-image: -moz-linear-gradient(top, #bbb, #ccc); /* FF3.6 */ background-image: -ms-linear-gradient(top, #bbb, #ccc); /* IE10 */ background-image: -o-linear-gradient(top, #bbb, #ccc); /* Opera 11.10+ */ background-image: linear-gradient(top, #bbb, #ccc); }

.btn:active, .btn.active { background-color: #bbb; background-image: none; outline: 0; -webkit-box-shadow: inset 0 0 1px #666; -moz-box-shadow: inset 0 0 1px #666; box-shadow: inset 0 0 1px #666; }

.btn.disabled { cursor: default; background-image: none; opacity: 0.65; filter: alpha(opacity=65); -webkit-box-shadow: 0px 0px 2px 2px #555; -moz-box-shadow: 0px 0px 2px 2px #555; box-shadow: 0px 0px 2px 2px #555; /* -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=180, Color='#000000')"; */ }

.btn-sm { font-size: 11px; padding: 0 6px; }

.btn-md { font-size: 13px; padding: 1px 15px; }

.btn-lg { padding: 10px 25px; }

.btn-login { color: #ffffff; font-size: 18px; font-weight: bold; padding: 12px 50px; background-color: #1aa2b9; /* Fallback Color */ background-image: -webkit-gradient(linear, left top, left bottom, from(#26c5e0), to(#1aa2b9)); /* Saf4+, Chrome */ background-image: -webkit-linear-gradient(top, #26c5e0, #1aa2b9); /* Chrome 10+, Saf5.1+, iOS 5+ */ background-image: -moz-linear-gradient(top, #26c5e0, #1aa2b9); /* FF3.6 */ background-image: -ms-linear-gradient(top, #26c5e0, #1aa2b9); /* IE10 */ background-image: -o-linear-gradient(top, #26c5e0, #1aa2b9); /* Opera 11.10+ */ background-image: linear-gradient(top, #26c5e0, #1aa2b9); }

.btn-login:hover, .btn-login:focus, .btn-login:active, .btn-login.active, .btn-login.disabled { color: #ffffff; background: #1aa2b9; }

.btn-primary { background: #14145E; color: #fff; font-weight: bold; }

.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .btn-primary.disabled { color: #ffffff; background: #1d1d88; }

.btn-red { background: #b44f00; color: #fff; font-weight: bold; }

.btn-red:hover, .btn-red:focus, .btn-red:active, .btn-red.active, .btn-red.disabled { color: #ffffff; background: #e76500; }
