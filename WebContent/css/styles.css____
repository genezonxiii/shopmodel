/* Login Styles
---------------------------------- */
.bkg-upper {
    position: absolute;
    top: 0;
    background: #e0f9fb;
    width: 85%;
    height: 50%;
    transform: skewX(-30deg);
}
.bkg-lower {
    position: absolute;
    bottom: 0;
    right: 0;
    background: #e0f9fb;
    width: 75%;
    height: 50%;
    transform: skewX(30deg);
}
.login-wrapper {
    position: absolute;
    width: 100%;
    top: 50%;
    margin-top: -260px;
}
.login-wrapper h1 {
    color: #62E0E9;
    font-size: 72px;
    font-weight: 100;
    letter-spacing: 10px;
    text-align: center;
    margin: 6px 0;
}
.login-panel-wrap {
    background: #62E0E9;
    padding: 10px 0 40px;
    margin-bottom: 15px;
    position: relative;
    zoom: 1;
}
.login-panel-wrap:before, .login-panel-wrap:after {
    content: '';
    display: table;
}
.login-panel-wrap:after {
    clear: both;
}
.login-panel-wrap:before {
    content: '';
    background: #fff;
    opacity: 0.5;
    display: block;
    width: 22%;
    position: absolute;
    top: 0;
    bottom: 100px;
    transform: skewX(30deg);
}
.login-panel {
    margin: 0 auto;
    width: 280px;
}
.login-panel h2 {
    color: #194A6B;
    font-weight: 100;
    letter-spacing: 23px;
    text-align: right;
    margin-bottom: 30px;
    position: relative;
}
.login-panel h2:after {
    content: '';
    background: #e0f9fb;
    display: block;
    height: 3px;
    position: absolute;
    bottom: -10px;
    width: 100%;
}
.login-panel label {
    color: #194A6B;
    display: block;
    font-weight: bold;
    margin-bottom: 20px;
    margin-right: 0;
}
.login-panel input[type=text], .login-panel input[type=password] {
    box-sizing: border-box;
    font-size: 24px;
    padding: 5px 15px;
    width: 100%;
}
.verify-wrap {
    zoom: 1;
}
.verify-wrap label {
    float: left;
    width: 50%;
}
.verify-wrap .captcha-wrap {
    float: right;
    width: 50%;
    padding-top: 24px;
    text-align: center;
}
.verify-wrap:before, .verify-wrap:after {
    content: '';
    display: table;
}
.verify-wrap:after {
    clear: both;
}
.login-button {
    background: #3F7FAA;
    border: 1px solid #3F7FAA;
    box-sizing: border-box;
    color: #fff;
    display: block;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    padding: 15px 0;
}
.login-button:hover {
    background: rgb(55, 111, 149);
}
.login-button:focus {
    background: rgb(28, 56, 74);
    border: 1px solid #194A6B;
}
.login-footer {
    color: #555;
    font-size: 13px;
    font-weight: bold;
    text-align: center;
    letter-spacing: 1px;
}
.login-footer span {
    font-weight: 100;
    margin-left: 1em;
}
/* Page Styles
---------------------------------- */
.page-wrapper {
    background: #3F7FAA;
    color: #333;
    width: 100%;
    height: 100%;
    overflow: hidden;
}
/* header
---------------------------------- */
.header {
    background-image: linear-gradient(#fff,#ddd);
    height: 56px;
    position: fixed;
    width: 100%;
}
.header h1 {
    font-size: 24px;
    font-weight: 100;
    float: left;
    margin-left: 32px;
    letter-spacing: 8px;
    line-height: 1.3;
}
.header .userinfo {
    float: right;
}
.header .userinfo p {
    color: #777;
    display: block;
    font-size: 15px;
    float: left;
    font-weight: 100;
    margin-right: 20px;
    padding-top: 1.3em;
}
.header .userinfo p span {
    color: #000;
    font-weight: bold;
    font-size: 18px;
    margin-left: 20px;
}
.header .btn-logout {
    background: rgb(253, 104, 104);
    color: #fff;
    display: block;
    float: left;
    font-size: 18px;
    text-align: center;
    line-height: 56px;
    height: 56px;
    width: 90px;
    text-decoration: none;
}
.header .btn-logout:hover {
    background: #FC3E3E;
}
/* 
sidenav ----------------------------------
*/

.sidenav {
    position: fixed;
    top: 56px;
    z-index: 9999;
}
.sidenav ul {
    width: 120px;
}
.sidenav ul li {
    display: block;
}
.sidenav ul li ul {
    background: #194A6B;
    box-shadow: 2px 1px 3px #777;
    min-width: 200px;
    position: absolute;
    left: 120px;
    top: 0;
    min-height: 100px;
    /*--toggle--*/
    opacity: 0;
    visibility: hidden;
    transition: all .3s linear;
}
.sidenav ul li ul li a {
    box-sizing: border-box;
    color: #fff;
    display: block;
    font-size: 16px;
    font-weight: 100;
    text-align: left;
    text-decoration: none;
    padding: 10px 20px;
    width: 200px;
}
.sidenav ul li ul li a:hover {
    background: #3F7FAA;
}
.sidenav > ul {}
.sidenav > ul > li {
    background: #f6f6f6;
    color: #777;
    font-size: 14px;
    width: 120px;
    height: 100px;
    text-align: center;
    position: relative;
}
.sidenav > ul > li img {
    height: 48px;
    display: block;
    margin: 0 auto;
    padding: 12px 0 12px;
}
.sidenav > ul > li.active {
    background-image: linear-gradient(#ddd,#eee,#d6fafb);
    color: #333;
}
.sidenav > ul > li:hover {
    background: #194A6B;
    color: #fff;
    transition: all .3s linear;
}
.sidenav > ul > li:hover ul {
    opacity: 1;
    visibility: visible;
}
/* content-wrap
---------------------------------- */
h2.page-title {
    /*background: var(--lightBlue);*/
    background-image: linear-gradient(#eee,#62E0E9);
    position: absolute;
    top: 0;
    left: 240px;
    height: 56px;
    letter-spacing: 5px;
    line-height: 56px;
    margin: 0;
    padding: 0 2em;
    position: fixed;
}
.content-wrap {
    background: #fff;
    float: left;
/*     margin-left: 120px; */
    margin-top: 56px;
    padding-bottom: 60px;
    height: calc(100vh - 142px);
    overflow: scroll;
/*     width: calc(100% - 120px); */
	width: 800px;
}
.input-field-wrap {
    background: #62E0E9;
    border-bottom: 3px solid #eee;
    padding: 20px 30px;
}
.input-field-wrap .form-wrap {
    text-align: center;
    margin: 0 auto;
}
.input-field-wrap .form-row .btn {
    vertical-align: bottom;
}
.input-field-wrap .btn-row {
    text-align: center;
    width: 100%;
}
.forward-mark {
    background: url(../images/icon-forward.svg) no-repeat;
    display: inline-block;
    width: 20px;
    height: 32px;
    margin: 0 5px 0 0px;
